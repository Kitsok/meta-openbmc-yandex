127.0.0.1       localhost.localdomain           localhost

# The following lines are desirable for IPv6 capable hosts
::1     localhost ip6-localhost ip6-loopback
fe00::0 ip6-localnet
ff00::0 ip6-mcastprefix
ff02::1 ip6-allnodes
ff02::2 ip6-allrouters

fd80:c001:beef::4ead:a  rmc-a
fd80:c001:beef::4ead:b  rmc-a

fd80:c001:beef::cb:1    cb-1
fd80:c001:beef::cb:2    cb-2
fd80:c001:beef::cb:3    cb-3
fd80:c001:beef::cb:4    cb-4
fd80:c001:beef::cb:5    cb-5
fd80:c001:beef::cb:6    cb-6

