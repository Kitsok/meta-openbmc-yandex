/dts-v1/;
/plugin/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/gpio/aspeed-gpio.h>

/ {
	compatible = "aspeed,ast2400";

	fragment@0 {
		target-path = "/";
		__overlay__ {
			model = "Gigabyte Shaosi RMC v3.0";
		};
	};

	fragment@1 {
		target = <&mac1>;
		__overlay__ {
			status = "okay";

			no-hw-checksum;
			phy-handle = <&phy1>;
			phy-mode = "rgmii";
			pinctrl-names = "default";
			pinctrl-0 = <&pinctrl_rgmii2_default &pinctrl_mdio2_default>;

			mdio {
				phy1: phy@0 {
					compatible = "ethernet-phy-id001c.c915";
					reg = <0>;
				};
			};
		};
	};

#if 0
	fragment@2 {
		target = <&i2c0>;
		__overlay__ {
			status = "okay";
			clock-frequency = <100000>;
			ipmb@1 {
				compatible = "i2c-ipmb";
				reg = <0x31>;
				slave-addr = <0x39>;
			};
		};
	};
#endif

	fragment@2 {
		target = <&i2c7>;
		__overlay__ {
			status = "okay";
			clock-frequency = <400000>;
			rtc@68 {
				compatible = "maxim,ds3231";
				reg = <0x68>;
				// interrupts = <GPIOF0>
			};

			tmp {
				label = "TEMP1";
				compatible = "national,lm75";
				reg = <0x48>;
			};
			fru {
				compatible = "atmel,24c256";
				reg = <0x56>;
				// pagesize = <32>;
			};
		};
	};
	fragment@3 {
		target = <&i2c0>;
		__overlay__ {
			status = "okay";
			/* RMCs virtual eeproms */
			rmca-eeprom {
				compatible = "atmel,24c02";
				reg = <0x39>;
			};
			rmcb-eeprom {
				compatible = "atmel,24c02";
				reg = <0x3b>;
			};
			/* CBs virtual eeproms */
			cb1-eeprom {
				compatible = "atmel,24c02";
				reg = <0x32>;
			};
			cb2-eeprom {
				compatible = "atmel,24c02";
				reg = <0x33>;
			};
			cb3-eeprom {
				compatible = "atmel,24c02";
				reg = <0x34>;
			};
			cb4-eeprom {
				compatible = "atmel,24c02";
				reg = <0x35>;
			};
			cb5-eeprom {
				compatible = "atmel,24c02";
				reg = <0x36>;
			};
			cb6-eeprom {
				compatible = "atmel,24c02";
				reg = <0x37>;
			};
		};
	};

};
