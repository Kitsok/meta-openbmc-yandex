/dts-v1/;
/plugin/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/gpio/aspeed-gpio.h>

/ {
	compatible = "aspeed,ast2400";

	fragment@0 {
		target-path = "/";
		__overlay__ {
			model = "Gigabyte Shaosi RMC v3.0";
		};
	};

	fragment@1 {
		target = <&mac1>;
		__overlay__ {
			status = "okay";

			no-hw-checksum;
			phy-handle = <&phy1>;
			phy-mode = "rgmii";
			pinctrl-names = "default";
			pinctrl-0 = <&pinctrl_rgmii2_default &pinctrl_mdio2_default>;

			mdio {
				phy1: phy@0 {
					compatible = "ethernet-phy-id001c.c915";
					reg = <0>;
				};
			};
		};
	};

#if 0
	fragment@2 {
		target = <&i2c0>;
		__overlay__ {
			status = "okay";
			clock-frequency = <100000>;
			ipmb@1 {
				compatible = "i2c-ipmb";
				reg = <0x31>;
				slave-addr = <0x39>;
			};
		};
	};
#endif

	fragment@2 {
		target = <&i2c7>;
		__overlay__ {
			status = "okay";
			clock-frequency = <400000>;
			rtc@68 {
				compatible = "maxim,ds3231";
				reg = <0x68>;
				// interrupts = <GPIOF0>
			};

			tmp {
				label = "TEMP1";
				compatible = "national,lm75";
				reg = <0x48>;
			};
			fru {
				compatible = "atmel,24c256";
				reg = <0x56>;
				// pagesize = <32>;
			};
		};
	};
	fragment@3 {
		target = <&i2c0>;
		__overlay__ {
			status = "okay";
			/* RMCs virtual eeproms */
			rmca-eeprom {
				compatible = "atmel,24c02";
				reg = <0x39>;
			};
			rmcb-eeprom {
				compatible = "atmel,24c02";
				reg = <0x3b>;
			};
			/* CBs virtual eeproms */
			cb1-eeprom {
				compatible = "atmel,24c02";
				reg = <0x32>;
			};
			cb2-eeprom {
				compatible = "atmel,24c02";
				reg = <0x33>;
			};
			cb3-eeprom {
				compatible = "atmel,24c02";
				reg = <0x34>;
			};
			cb4-eeprom {
				compatible = "atmel,24c02";
				reg = <0x35>;
			};
			cb5-eeprom {
				compatible = "atmel,24c02";
				reg = <0x36>;
			};
			cb6-eeprom {
				compatible = "atmel,24c02";
				reg = <0x37>;
			};
		};
	};
	fragment@4 {
		target = <&i2c0>;
		__overlay__ {
			status = "okay";
			bpbio: gpio-expander@18 {
				compatible = "nxp,pca9675";
				gpio-controller;
				#gpio-cells = <2>;
				reg = <0x18>;
				lines-initial-states = <0x1>;
			};
			psumux@70 {
				compatible = "nxp,pca9547";
				#address-cells = <1>;
				#size-cells = <0>;
				reg = <0x70>;
				reset-gpios = <&bpbio 0 GPIO_ACTIVE_LOW>;
				i2c-mux-idle-disconnect = <0xff>;
				shelf@0 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <0>;
					fru {
						compatible = "atmel,24c02";
						reg = <0x50>;
					};
				};
				shelf@1 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <1>;
					fru {
						compatible = "atmel,24c02";
						reg = <0x50>;
					};
				};
				shelf@2 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <2>;
					fru {
						compatible = "atmel,24c02";
						reg = <0x50>;
					};
				};
				shelf@3 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <3>;
					fru {
						compatible = "atmel,24c02";
						reg = <0x50>;
					};
				};
				shelf@4 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <4>;
					fru {
						compatible = "atmel,24c02";
						reg = <0x50>;
					};
				};
				shelf@5 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <5>;
					fru {
						compatible = "atmel,24c02";
						reg = <0x50>;
					};
				};

			};
		};
	};

};
